{#    <script src="{{ asset('assets/plugins/bootstrap-datepicker/js/bootstrap-datepicker.min.js') }}" type="text/javascript"></script>#}
    <script>
        var trn_circle_appUser = '';
        var $pageURL = $(location).attr("href");
        var possession_date = $("#trn_project_possessionDate");
        var possession_year = $("#trn_project_possessionYear");
        var possession_month = $("#trn_project_possessionMonth");
        $(function () {

            possession_date.on("change",function(){
                if ($(this).val() !== '') {
                    possession_year.attr("value","");
                    possession_month.attr("value","");
                    possession_year.prop("disabled", true);
                    possession_month.prop("disabled", true);
                } else {
                    possession_year.prop("disabled", false);
                    possession_month.prop("disabled", false);
                }
            });
            possession_year.on("change",function(){
                if ($(this).val() !== '') {
                    possession_date.val("");
                    possession_date.prop("disabled", true);
                } else {
                    possession_date.prop("disabled", false);
                }
