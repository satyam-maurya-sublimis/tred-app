            <div class="col-sm-2">
                {{ form_row(form.mstSubCategory) }}
            </div>
            <div class="col-sm-2">
                {{ form_row(form.mstProjectAmenities) }}
            </div>
            <div class="col-sm-4">
                {{ form_row(form.trnAmenitiesDescription) }}
            </div>
            <div class="col-sm-2">
                {{ form_row(form.position) }}
            </div>
            <div class="col-sm-1">
                {{ form_row(form.isActive) }}
            </div>
            <div class="col-sm-1 removeAmenitiesContentClass mb-4"> <br>
                <button type="button" class="btn btn-danger btn-sm"><i class="fa fa-trash"></i> Remove</button>
            </div>
        </div>
