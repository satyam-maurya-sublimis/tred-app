{% extends 'portal/layout/index.html.twig' %}
{% set office = get_office_list() %}
{% block stylesheets %}
    {{ parent() }}
    <link href="{{ asset('assets/plugins/summernote/summernote-bs4.css') }}" media="all" rel="stylesheet" type="text/css" />
    <link rel="stylesheet" href="{{asset('assets/plugins/kartik-v/bootstrap-fileinput/css/fileinput.min.css')}}">
{% endblock %}
{% block portal_content %}
    {% set price =[] %}
    {% set noOfBedRoom =[] %}
    {% set noOfBathRooms =[] %}
    {% set propertyTransactionCategory =[] %}
    {%  for key,roomConfiguration in roomConfigurations %}
        {% set price = {("roomId_"~roomConfiguration.id):roomConfiguration.agreementAmount}|merge(price) %}
        {% if roomConfigurations.mstPropertyTransactionCategory.propertyTransactionCategory not in propertyTransactionCategory %}
            {% set propertyTransactionCategory = propertyTransactionCategory|merge([roomConfigurations.mstPropertyTransactionCategory.propertyTransactionCategory]) %}
        {% endif %}
        {% if roomConfiguration.noOfBathRooms not in noOfBathRooms %}
            {% set noOfBathRooms = noOfBathRooms|merge([roomConfiguration.noOfBathRooms]) %}
        {% endif %}
        {% if roomConfiguration.noOfBedRoom not in noOfBedRoom %}
            {% set noOfBedRoom = noOfBedRoom|merge([roomConfiguration.noOfBedRoom]) %}
        {% endif %}
    {% endfor %}
    {% set priceGroup = []|merge({("id_"~roomConfigurations.trnProject.id): price|sort }) %}
    {% set additionalDetails =[] %}
    {% set projectAmenities = [] %}
    {% set projectAmenitiesIcons = [] %}
    {% set projectAmenitiesCategories = [] %}
    {%  for trnProjectAdditionalDetail in roomConfigurations.trnProjectAdditionalDetails %}
        {% if trnProjectAdditionalDetail.additionalDetailType not in additionalDetails %}
            {% set additionalDetails = [trnProjectAdditionalDetail.additionalDetailType]|merge(additionalDetails) %}
        {% endif  %}
    {% endfor %}
    {% for amenities in roomConfigurations.trnProject.trnProjectAmenities|filter((a)=>a.isActive) %}
        {% if ("%02d"|format(amenities.mstSubCategory.id)~"|"~amenities.mstSubCategory.subCategory~"|"~"%02d"|format(amenities.id)~"|"~amenities.trnAmenitiesDescription~"|"~"%02d"|format(amenities.mstProjectAmenities.id)~"|"~amenities.mstProjectAmenities
            ) not in projectAmenities %}
            {% set projectAmenities = ["%02d"|format(amenities.mstSubCategory.id)~"|"~amenities.mstSubCategory.subCategory~"|"~"%02d"|format(amenities.id)~"|"~amenities.trnAmenitiesDescription~"|"~"%02d"|format(amenities.mstProjectAmenities.id)~"|"~amenities.mstProjectAmenities]|merge(projectAmenities) %}
        {% endif %}
        {% if ("%02d"|format(amenities.mstSubCategory.id)~"|"~amenities.mstSubCategory.subCategory) not in projectAmenitiesCategories %}
            {% set projectAmenitiesCategories = ["%02d"|format(amenities.mstSubCategory.id)~"|"~amenities.mstSubCategory.subCategory]|merge(projectAmenitiesCategories) %}
        {% endif %}
        {% if amenities.mstProjectAmenities.mediaIcon %}
            {% if attribute(projectAmenitiesIcons,"id_"~amenities.id) is not defined %}
                {% set projectAmenitiesIcons = {("id_"~amenities.id) : amenities.mstProjectAmenities.mediaIcon.iconImage}|merge(projectAmenitiesIcons) %}
            {% endif %}
        {% endif  %}
    {% endfor %}
    {% set similarProjects = get_other_project(roomConfigurations.trnProject.trnVendorPartnerDetails,roomConfigurations.trnProject.id) %}
    <div class="wrapper">
        <section>
            <div class="container">
                <nav aria-label="breadcrumb">
                    <ol class="breadcrumb">
                        <li class="breadcrumb-item"><a href="{{ path('portal_index') }}">Home</a></li>
                        {% if roomConfigurations.mstPropertyTransactionCategory.propertyTransactionCategory == "Buy" %}
                            <li class="breadcrumb-item"><a href="{{ path('portal_properties_buy_list',{'slugName': roomConfigurations.trnProject.mstProductType.productType|lower }) }}">{{ roomConfigurations.mstPropertyTransactionCategory.propertyTransactionCategory }} - {{ roomConfigurations.trnProject.mstProductType.productType }} Projects in {{ roomConfigurations.trnProject.mstCity.city }}</a></li>
                        {% else %}
                            <li class="breadcrumb-item"><a href="{{ path('portal_properties_rent_list',{'slugName': roomConfigurations.trnProject.mstProductType.productType|lower }) }}">{{ roomConfigurations.mstPropertyTransactionCategory.propertyTransactionCategory }} - {{ roomConfigurations.trnProject.mstProductType.productType }} Projects in {{ roomConfigurations.trnProject.mstCity.city }}</a></li>
                        {% endif %}
                        {#                        <li class="breadcrumb-item"><a href="{{ path('portal_properties_buy_list',{'slugName':roomConfigurations.trnProject.mstProductType }) }}">{{ roomConfigurations.trnProject.mstProductType }}</a></li>#}
                        {#                        <li class="breadcrumb-item"><a href="#">Property In {{ roomConfigurations.trnProject.mstCity }}</a></li>#}
                        <li class="breadcrumb-item active" aria-current="page">{{ roomConfigurations.trnProject.projectName }}</li>
                    </ol>
                </nav>
            </div>
        </section>
        <section class="flat-dtls-bar mt-2">




                    <div class="container">
                        <ul class="flat-dtls">
                            {% if roomConfigurations.mstPropertyTransactionCategory.id == 1 %}
                                <li>
                                    <div>
                                        <b>{{ roomConfigurations.noOfBedRoom }} BHK {{ roomConfigurations.trnProject.mstProductSubType.productSubType }}</b> in {{ roomConfigurations.trnProject.projectName }}
                                        <span class="location"><b>{{ roomConfigurations.trnProject.trnVendorPartnerDetails.vendorPartnerName }},</b> {{ roomConfigurations.trnProject.mstPincode.officeName }}, {{ roomConfigurations.trnProject.mstCity.city }} - {{ roomConfigurations.trnProject.mstPincode }}</span>
                                    </div>
                                    <ul>
                                        <li>
                                            <div>
                                                <small>{{ roomConfigurations.mstProjectAreaCategory }}</small>
                                                <span><b class="amt">{{ roomConfigurations.areaValue }}</b> {{ roomConfigurations.mstProjectArea }}</span>
                                            </div>
                                        </li>
                                        <li>
                                            <div class="prz">
                                    <span>
                                      <i class="fa fa-rupee"></i>
                                      <b>
                                          {% if roomConfigurations.mstPropertyTransactionCategory.id == 1 %}
                                          {{ roomConfigurations.agreementAmount|replace({'.00':'' }) }} </b> {{ roomConfigurations.mstDenomination.abbreviation }}
                                        {% else %}
                                            {{ roomConfigurations.rentPerMonth }}
                                        {% endif %}
                                    </span>
                                                <small>onwards</small>
                                            </div>
                                        </li>
                                    </ul>
                                </li>

                            {% else %}
                                <li>

                                    <div>
                                        <b>{{ roomConfigurations.noOfBedRoom }} BHK {{ roomConfigurations.trnProject.mstProductSubType.productSubType }}</b> For Rent in {{ roomConfigurations.trnProject.projectName }}
                                        <span class="location"><b>{{ roomConfigurations.trnProject.trnVendorPartnerDetails.vendorPartnerName }},</b> {{ roomConfigurations.trnProject.mstPincode.officeName }}, {{ roomConfigurations.trnProject.mstCity.city }} - {{ roomConfigurations.trnProject.mstPincode }}</span>
                                    </div>
                                    <ul>
                                        <li>
                                            <div class="prz">
                                                <span>
                                                  <i class="fa fa-rupee"></i>
                                                  <b>{{ roomConfigurations.rentPerMonth|replace({'.00':''}) }}</b>/M
                                                </span>
                                                <small>{{ roomConfigurations.isNegotiable?"Negotiable":"" }}</small>
                                            </div>
                                        </li>
                                        <li>
                                            <div class="prz">
                                                <span>
                                                  <b>{{ roomConfigurations.areaValue }}</b>
                                                </span>
                                                <small>{{ roomConfigurations.mstProjectArea }}</small>
                                            </div>
                                        </li>
                                        <li>
                                            <div class="prz">
                                                <span>
                                                  <i class="fa fa-rupee"></i>
                                                  <b>{{ roomConfigurations.deposit|replace({'.00':''}) }} {{ roomConfigurations.mstDenomination }}</b>
                                                </span>
                                                <small>Deposit</small>
                                            </div>
                                        </li>
                                    </ul>
                                </li>
                            {% endif %}
                            <li>
                                <ul>
                                    <li class="share">
                                        <div>
                                            <a href="#" class="fa fa-share-alt"></a>
                                        </div>
                                        <div class="share-widget">
                                            <label>Share On</label>
                                            <ul>
                                                <li><a href="https://www.facebook.com/sharer/sharer.php?u={{ app.request.schemeAndHttpHost ~ app.request.requestUri }}" target="_blank"><i class="fa fa-facebook"></i></a></li>
                                                {#                                                <li><a href="#" class="fa-instagram"></a></li>#}
                                                <li><a href="https://www.linkedin.com/shareArticle?mini=true&title={{ roomConfigurations.trnProject.projectName }}&url={{ app.request.schemeAndHttpHost ~ app.request.requestUri }}" target="_blank"><i class="fa fa-linkedin"></i></a></li>
                                                <li><a href="https://api.whatsapp.com/send?phone=918291890911&text={{ app.request.schemeAndHttpHost ~ app.request.requestUri }}" target="_blank"><i class="fa fa-whatsapp"></i></a></li>
                                            </ul>
                                            <a class="close-share" href="javascript:;"><i class="fa fa-times"></i></a>
                                        </div>
                                    </li>
                                </ul>
                            </li>
                        </ul>
                    </div>

        </section>
        <section class="mt-2">
            <div class="container">
                <div class="row">
                    <div class="col-md-9">
                        {% set uploadImageCount = roomConfigurations.trnUploadDocuments|length %}
                        <div id="sync1" class="slider owl-carousel">
                            {% for  upload in roomConfigurations.trnUploadDocuments|sort((a,b)=>a.position <=> b.position) %}
                                <div class="item">
                                    <div class="card-deck cust-card">
                                        <div class="card">
                                            <figure class="">
                                                <img src="{{ upload.mediaFilePath|imagine_filter('gallery_image_image_900_450') }}" class="card-img-top" alt="...">
                                                <figcaption>
                                                    <div class="top-left">
                                                        {% if roomConfigurations.trnProject.isTredRecommended %}
                                                            <span><i class="fa fa-check"></i> {{ roomConfigurations.trnProject.orgCompany.company }} Recommended </span>
                                                        {% endif  %}
                                                    </div>

                                                    {#                                                <div class="center">#}
                                                    {#                                                    <a href="#" class="play-icn">#}
                                                    {#                                                        <i class="fa fa-play"></i>#}
                                                    {#                                                        <small>3d View</small>#}
                                                    {#                                                    </a>#}
                                                    {#                                                </div>#}
                                                    <div class="bottom-left">
                                                        <ul class="like-view">
                                                            <li>
                                                                <a href="#" data-id="{{ roomConfigurations.id }}" class="thumbs-up" >
                                                                    <i class="fa fa-thumbs-o-up"></i>
                                                                    <b class="fa-thumbs-o-up-count">{% if roomConfigurations.roomConfigurationLikes %}{{ roomConfigurations.roomConfigurationLikes }}{% else %}0{% endif %}</b>
                                                                </a>
                                                            </li>
                                                            <li>
                                                                <a href="#">
                                                                    <i class="fa fa-eye"></i>
                                                                    <b>{% if roomConfigurations.roomConfigurationViews %}{{ roomConfigurations.roomConfigurationViews }}{% else %}0{% endif %}</b>
                                                                </a>
                                                            </li>
                                                        </ul>
                                                    </div>
                                                    {% if roomConfigurations.trnProject.isRera %}
                                                        <div class="bottom-right">
                                                            <div class="rera">
                                                                RERA
                                                            </div>
                                                        </div>
                                                    {% endif  %}
                                                </figcaption>
                                            </figure>
                                        </div>
                                    </div>
                                </div>
                            {% else %}
                                <div class="item">
                                    <div class="card-deck cust-card">
                                        <div class="card">
                                            <figure class="">
                                                <img src="{{ asset('resources/images/no_image.jpg') }}" class="card-img-top" title="No Image" alt="No Image">
                                                <figcaption>
                                                    <div class="top-left">
                                                        {% if roomConfigurations.trnProject.isTredRecommended %}
                                                            <span><i class="fa fa-check"></i> {{ roomConfigurations.trnProject.orgCompany.company }} Recommended </span>
                                                        {% endif  %}
                                                    </div>

                                                    {#                                                <div class="center">#}
                                                    {#                                                    <a href="#" class="play-icn">#}
                                                    {#                                                        <i class="fa fa-play"></i>#}
                                                    {#                                                        <small>3d View</small>#}
                                                    {#                                                    </a>#}
                                                    {#                                                </div>#}
                                                    <div class="bottom-left">
                                                        <ul class="like-view">
                                                            <li>
                                                                <a href="#" data-id="{{ roomConfigurations.trnProject.id }}" class="thumbs-up" >
                                                                    <i class="fa fa-thumbs-o-up"></i>
                                                                    <b class="fa-thumbs-o-up-count">{% if roomConfigurations.trnProject.projectlikes %}{{ roomConfigurations.trnProject.projectlikes }}{% else %}0{% endif %}</b>
                                                                </a>
                                                            </li>
                                                            <li>
                                                                <a href="#">
                                                                    <i class="fa fa-eye"></i>
                                                                    <b>{% if roomConfigurations.trnProject.projectViews %}{{ roomConfigurations.trnProject.projectViews }}{% else %}0{% endif %}</b>
                                                                </a>
                                                            </li>
                                                        </ul>
                                                    </div>
                                                    {% if roomConfigurations.trnProject.isRera %}
                                                        <div class="bottom-right">
                                                            <div class="rera">
                                                                RERA
                                                            </div>
                                                        </div>
                                                    {% endif  %}
                                                </figcaption>
                                            </figure>
                                        </div>
                                    </div>
                                </div>
                            {% endfor %}
                            <div class="item">
                                <a href="#" class="btn-view-more">
                                    <i class="fa fa-camera"></i>
                                    <b>{{ uploadImageCount }} Photos</b>
                                </a>
                            </div>
                        </div>
                        <div id="sync2" class="navigation-thumbs owl-carousel">
                            {% for  upload in roomConfigurations.trnUploadDocuments|sort((a,b)=>a.position <=> b.position) %}
                                <div class="item">
                                    <img src="{{ upload.mediaFilePath|imagine_filter('gallery_image_image_100_80') }}" class="card-img-top" alt="...">
                                </div>
                            {% endfor %}
                        </div>
                        {% if roomConfigurations.mstPropertyTransactionCategory.propertyTransactionCategory != "Buy" %}
                            <div class="rent-details">
                                <ul class="flat-dtls property-dtls">
                                    <li>
                                <span class="icon"><img src="/resources/images/icons/icon-Furniture-sm.png">
                                </span>
                                        <span>
                                        {{ noOfBedRoom|join(',') }}
                                    <small>No. of Bedroom </small>
                                </span>
                                    </li>
                                    <li>
                                <span class="icon"><img src="/resources/images/icons/icon-property-sm.png">
                                </span>
                                        <span>
                                    {{ roomConfigurations.trnProject.mstProductSubType }}
                                    <small>Property Type</small>
                                </span>

                                    </li>
                                    <li>
                                <span class="icon"><img src="/resources/images/icons/icon-Family.png">
                                </span>
                                        <span>
                                    {% set preferedTenants = '' %}
                                            {% for roomConfiguration in roomConfigurations %}
                                                {% set preferedTenants = roomConfiguration.mstPreferredTenant|join(',') %}
                                            {% endfor %}
                                            {{ preferedTenants}}
                                    <small>Preferred Tenants</small>
                                </span>

                                    </li>
                                    <li>
                                <span class="icon"><i class="fa fa-calendar-o"></i>
                                </span>
                                        <span>
                                    {{ roomConfigurations.trnProject.availabilityFromDate|date('d M Y') }}
                                    <small>Possession</small>
                                </span>
                                    </li>
                                    <li>
                                <span class="icon"><img src="/resources/images/icons/Icon-Parking.png">
                                </span>
                                        <span>
                                  None
                                    <small>Parking</small>
                                </span>
                                    </li>
                                    <li>
                                <span class="icon"><img src="/resources/images/icons/icon-AgeProperty.png">
                                </span>
                                        <span>
                                    {{ roomConfigurations.trnProject.propertyAge }}
                                    <small>Age of Property</small>
                                </span>

                                    </li>
                                    <li>
                                <span class="icon"><img src="/resources/images/icons/Icon-Balcony.png">
                                </span>
                                        <span>
                                    {% set balcony = '' %}
                                            {% for roomConfiguration in roomConfigurations %}
                                                {% set balcony = roomConfiguration.noOfBalcony|join(',') %}
                                            {% endfor %}
                                            {{ balcony }}
                                    <small>Balcony</small>
                                </span>

                                    </li>
                                    <li>
                                <span class="icon"><i class="fa fa-flag-o"></i>
                                </span>
                                        <span>
                                    {{ roomConfigurations.trnProject.availabilityFromDate|date('d M Y') }}
                                    <small>Posted On</small>
                                </span>
                                    </li>
                                </ul>
                            </div>
                        {% endif  %}
                        <div class="scroll-container">
                            <div class="project-dtls">
                                {% if roomConfigurations.mstPropertyTransactionCategory.propertyTransactionCategory == "Buy" %}
                                    <div id="secondary-nav" class="secondary-nav">
                                        <nav class="vam nav scroll dragger scroller" id="main-menu">
                                            <li class="nav-item"><a class="nav-link active" href="#overview" data-id="overview" aria-controls="overview">Overview</a></li>
                                            <li class="nav-item"><a class="nav-link" href="#floor-plan">Floor Plan</a></li>
                                            {% if roomConfigurations.trnProject.mstProjectFeature|length %} <li class="nav-item"><a class="nav-link" href="#features">Features</a></li> {% endif %}
                                            {% if projectAmenities|length %}
                                                <li class="nav-item"><a class="nav-link" href="#amenities">Amenities</a></li>
                                            {% endif  %}
                                            <li class="nav-item"><a class="nav-link" href="#project-details">Project Details</a></li>
                                            {% if 'surroundings' in additionalDetails %}
                                                <li class="nav-item"><a class="nav-link" href="#surroundings">Surroundings</a></li>
                                            {% endif  %}
                                            {% if 'price_and_payment_terms' in additionalDetails %}
                                                <li class="nav-item"><a class="nav-link" href="#price">Price</a></li>
                                            {% endif  %}
                                            {% if 'terms_and_conditions' in additionalDetails %}
                                                <li class="nav-item"><a class="nav-link" href="#tnc">Terms & Conditions</a></li>
                                            {% endif  %}
                                            {% if 'contact_details' in additionalDetails %}
                                                <li class="nav-item"><a class="nav-link" href="#contact-details">Contact Details</a></li>
                                            {% endif  %}
                                            {% if 'general' in additionalDetails %}
                                                <li class="nav-item"><a class="nav-link" href="#general">General</a></li>
                                            {% endif  %}
                                            {% if similarProjects|length %}
                                                <li class="nav-item"><a class="nav-link" href="#similar-projects">Similar Projects</a></li>
                                            {% endif %}
                                        </nav>
                                    </div>
                                {% endif %}
                                <div  class="">
                                    <div id="overview" class="section scrollspy">
                                        {% if roomConfigurations.mstPropertyTransactionCategory.propertyTransactionCategory != "Buy" %}
                                            <h2>Overview</h2>
                                        {% endif  %}
                                        <ul class="overview-list">


                                                {% if roomConfigurations.mstPropertyTransactionCategory.id == 1  %}
                                                    <li>Project Type<b>{{ roomConfigurations.trnProject.mstPropertyType }}</b></li>
                                                    <li>Project Status<b>{{ roomConfigurations.trnProject.mstProjectStatus.propertyType }}</b></li>
                                                    <li>Possession on
                                                        <b>
                                                            {% if roomConfigurations.trnProject.possessionDate != null %}
                                                                {{ roomConfigurations.trnProject.possessionDate|date('M - Y') }}
                                                            {% elseif roomConfigurations.trnProject.possessionYear != "" and roomConfigurations.trnProject.possessionMonth  %}
                                                                {{ roomConfigurations.trnProject.possessionMonth[:3] }} - {{ roomConfigurations.trnProject.possessionYear }}
                                                            {% endif %}
                                                        </b>
                                                    </li>
                                                    <li>Configurations<b>{{ roomConfigurations.mstRoomConfiguration }}</b></li>
                                                    <li>Developer<b>{{ roomConfigurations.trnProject.trnVendorPartnerDetails }}</b></li>
                                                    <li>Project <b>{{ roomConfigurations.trnProject.projectName }}</b></li>
                                                    <li>{{ roomConfigurations.mstProjectAreaCategory }}<b>{{ roomConfigurations.areaValue }} {{ roomConfigurations.mstProjectArea }}</b><small>₹ {{ roomConfigurations.price }}/{{ roomConfigurations.mstProjectArea }}</small></li>
                                                    {#                                                <li>Carpet area<b>700 sqft</b><small>₹ 41,125/sqft</small></li>#}
                                                    {#                                                <li>Loading<b>39%</b></li>#}
                                                    {#                                                <li>Floor<b>49 (Out of 65 Floors)</b></li>#}
                                                    <li>Car parking<b>{{ roomConfigurations.trnProject.carParking }}</b></li>
                                                    <li>RERA ID
                                                        <b>
                                                            {% if roomConfigurations.trnProject.isRera %}
                                                                {% if roomConfigurations.trnProject.mahaReraRegisterationNo %}
                                                                    {{ roomConfigurations.trnProject.mahaReraRegisterationNo }}
                                                                {% else %}
                                                                    Yes
                                                                {% endif %}
                                                            {% else %}
                                                                No
                                                            {% endif %}
                                                        </b></li>
                                                    <li>Bedrooms<b>{{ roomConfigurations.noOfBedRoom }}</b></li>
                                                    <li>Bathrooms<b>{{ roomConfigurations.noOfBathRooms }}</b></li>
                                                    <li>Bank Approved <b>{{ roomConfigurations.trnProject.bankApproved ? 'Yes' : 'No' }}</b></li>
                                                {% elseif roomConfigurations.mstPropertyTransactionCategory.id == 2 %}
                                                    <li>Project Type<b>{{ roomConfigurations.trnProject.mstPropertyType }}</b></li>
                                                    <li>Floor<b>{{ roomConfigurations.floor }}</b></li>
                                                    <li>Furnished Status<b>{{ roomConfigurations.mstFurnishing }}</b></li>
                                                    <li>Status of Electricity<b>{{ roomConfigurations.trnProject.electricityStatus }}</b></li>
                                                    <li>Bathrooms <b>{{ noOfBathRooms|join(',') }}</b></li>
                                                    <li>Lifts <b>{{ roomConfigurations.trnProject.noOfLifts  }}</b></li>
                                                    <li>Facing <b>{{ roomConfigurations.mstFacing }}</b></li>
                                                    <li>Water Availability <b>{{ roomConfigurations.trnProject.waterAvailibility  }}</b></li>
                                                {% endif %}

                                        </ul>
                                    </div>
                                    {% if roomConfigurations.mstPropertyTransactionCategory.propertyTransactionCategory == "Buy" %}
                                        <div id="floor-plan" class="floor-plan section scrollspy">
                                            <h2>Floor Plan</h2>
                                            <ul class="nav nav-tabs h-tabs" id="myTab" role="tablist">
                                                {% set floorPlan = [] %}
                                                {% for roomConfiguration in roomConfigurations.trnProject.trnProjectRoomConfigurations %}
                                                    {% if roomConfigurations.createdBy.id == roomConfiguration.createdBy.id %}
                                                        {% if (roomConfiguration.mstroomConfiguration.roomConfiguration~"|"~"%02d"|format(roomConfiguration.mstroomConfiguration.id)~"|"~"%02d"|format(roomConfiguration.createdBy.id)) not in floorPlan %}
                                                            {% set floorPlan = [roomConfiguration.mstroomConfiguration.roomConfiguration~"|"~"%02d"|format(roomConfiguration.mstroomConfiguration.id)~"|"~"%02d"|format(roomConfiguration.createdBy.id)]|merge(floorPlan) %}
                                                        {% endif %}
                                                    {% endif %}
                                                {% endfor %}
                                                {% for fplan in floorPlan|sort %}
                                                    {% set tmpfplan = fplan|split("|") %}
                                                    <li class="nav-item" role="presentation">
                                                        <a class="nav-link {% if tmpfplan.1 == roomConfigurations.mstroomConfiguration.id %}active{% endif %}" id="tab-{{ loop.index }}" data-toggle="tab" href="#tab{{ loop.index }}" role="tab" aria-controls="home" aria-selected="true">{{ tmpfplan.0 }}</a>
                                                    </li>
                                                {% endfor %}
                                            </ul>
                                            <div class="tab-content tabs-container" id="myTabContent">
                                                {% for fplan in floorPlan|sort %}
                                                    {% set tmpfplan = fplan|split("|") %}

                                                    <div class="tab-pane fade show {% if tmpfplan.1 == roomConfigurations.mstroomConfiguration.id %}active{% endif %}" id="tab{{ loop.index }}" role="tabpanel" aria-labelledby="tab-{{ loop.index }}">
                                                        <table class="table table-borderless flpln-dtls">
                                                            <thead>
                                                            <tr>
                                                                <th>Floor Blue Print</th>
                                                                <th>Area</th>
                                                                <th>Agreement Price</th>
                                                            </tr>
                                                            </thead>
                                                            <tbody>
                                                            {% for roomConfiguration in roomConfigurations.trnProject.trnProjectRoomConfigurations|filter((a)=>a.mstroomConfiguration.id == tmpfplan.1 and a.createdBy.id == tmpfplan.2 and a.mstPropertyTransactionCategory.propertyTransactionCategory =="Buy" ) %}
                                                                <tr>
                                                                    <td>
