{% extends 'portal/layout/index.html.twig' %}
{% set route =  app.request.get('_route') %}
{% if route == 'portal_properties_buy_list_status' %}
    {% set route =  'portal_properties_buy_list' %}
{% endif  %}
{% if route == 'portal_properties_rent_list_status' %}
    {% set route =  'portal_properties_rent_list' %}
{% endif  %}
{% set cmsPage = get_cms_page_by_route(route) %}

{% set ads = [] %}
{% if cmsPage %}
    {% set ads = get_cms_page_ads(cmsPage.id) %}
{% endif  %}
{% block portal_content %}
    {% set projectStatuses = [] %}
    {% set projectRoomConfigurations = [] %}
    {% set projectProjectType = [] %}
    {% set projectPossessions = [] %}
    {% set projectAmenities = [] %}
    {% set projectAmenitiesIcons = [] %}
    {% set projectAmenitiesCategories = [] %}
    {% set projectProductSubType = [] %}
    {% set projectPostedBy = [] %}
    {% set projectMoreFilter = [] %}
    {% set noOfBathRooms = [] %}
    {% set projectArea = [] %}
    {% set projectAreaValue = [] %}
    {% set projectPrice = [] %}
    {% set projectPriceGroup = [] %}

    {% for project in projects %}
        {% if loop.first %}
{#            {{ dump(project) }}#}
        {% endif %}
        {% if ("%02d"|format(project.mstProjectStatus.id)~"|"~project.mstProjectStatus.propertyType) not in projectStatuses %}
            {% set projectStatuses = ["%02d"|format(project.mstProjectStatus.id)~"|"~project.mstProjectStatus.propertyType]|merge(projectStatuses) %}
        {% endif %}

        {% if ("%02d"|format(project.mstPropertyType.id)~"|"~project.mstPropertyType.projectType) not in projectProjectType %}
            {% set projectProjectType = ["%02d"|format(project.mstPropertyType.id)~"|"~project.mstPropertyType.projectType]|merge(projectProjectType) %}
        {% endif %}
        {% if ("%02d"|format(project.mstPropertyType.id)~"|"~"%02d"|format(project.mstProductSubType.id)~"|"~project.mstProductSubType.productSubType) not in projectProductSubType %}
            {% set projectProductSubType = ["%02d"|format(project.mstPropertyType.id)~"|"~"%02d"|format(project.mstProductSubType.id)~"|"~project.mstProductSubType.productSubType]|merge(projectProductSubType) %}
        {% endif %}

        {% if project.mstPossession %}
            {% if ("%02d"|format(project.mstPossession.id)~"|"~project.mstPossession.possession) not in projectPossessions %}
                {% set projectPossessions = ["%02d"|format(project.mstPossession.id)~"|"~project.mstPossession.possession]|merge(projectPossessions) %}
            {% endif %}
        {% endif %}
        {% for rooms in project.mstRoomConfiguration %}
                {% if ("%02d"|format(rooms.id)~"|"~rooms.roomConfiguration) not in projectRoomConfigurations %}
                    {% set projectRoomConfigurations = ["%02d"|format(rooms.id)~"|"~rooms.roomConfiguration]|merge(projectRoomConfigurations) %}
                {% endif %}
        {% endfor%}

        {% for amenities in project.trnProjectAmenities|filter((a)=>a.isActive) %}
            {% if ("%02d"|format(amenities.mstSubCategory.id)~"|"~amenities.mstSubCategory.subCategory~"|"~"%02d"|format(amenities.mstProjectAmenities.id)~"|"~amenities.mstProjectAmenities) not in projectAmenities %}
                {% set projectAmenities = ["%02d"|format(amenities.mstSubCategory.id)~"|"~amenities.mstSubCategory.subCategory~"|"~"%02d"|format(amenities.mstProjectAmenities.id)~"|"~amenities.mstProjectAmenities]|merge(projectAmenities) %}
            {% endif %}
            {% if ("%02d"|format(amenities.mstSubCategory.id)~"|"~amenities.mstSubCategory.subCategory) not in projectAmenitiesCategories %}
                {% set projectAmenitiesCategories = ["%02d"|format(amenities.mstSubCategory.id)~"|"~amenities.mstSubCategory.subCategory]|merge(projectAmenitiesCategories) %}
            {% endif %}
            {% if amenities.mstProjectAmenities.mediaIcon %}
                {% if attribute(projectAmenitiesIcons,"id_"~amenities.id) is not defined %}
                    {% set projectAmenitiesIcons = {("id_"~amenities.id) : amenities.mstProjectAmenities.mediaIcon.iconImage}|merge(projectAmenitiesIcons) %}
                {% endif %}
            {% endif  %}
        {% endfor%}
        {% if project.trnVendorPartnerDetails|length > 0 %}
            {% if ("%02d"|format(project.trnVendorPartnerDetails.mstVendorType.id)~"|"~project.trnVendorPartnerDetails.mstVendorType.vendorType) not in projectPostedBy %}
                {% set projectPostedBy = ["%02d"|format(project.trnVendorPartnerDetails.mstVendorType.id)~"|"~project.trnVendorPartnerDetails.mstVendorType.vendorType]|merge(projectPostedBy) %}
            {% endif %}
        {% endif %}
        {% if project.mstProductSubType %}
            {% if ("%02d"|format(project.mstProductSubType.id)~"|"~project.mstProductSubType.productSubType) not in projectProductSubType %}
                {% set projectProductSubType = ["%02d"|format(project.mstProductSubType.id)~"|"~project.mstProductSubType.productSubType]|merge(projectProductSubType) %}
            {% endif %}
        {% endif %}

        {% for trnProjectRoomConfigurations in project.trnProjectRoomConfigurations %}
                {% if trnProjectRoomConfigurations.mstPropertyTransactionCategory.id == transactionCategoryId %}
                    {% if (trnProjectRoomConfigurations.noOfBathRooms) not in noOfBathRooms %}
                        {% set noOfBathRooms = noOfBathRooms|merge([trnProjectRoomConfigurations.noOfBathRooms]) %}
                    {% endif  %}
                    {% if (trnProjectRoomConfigurations.mstProjectArea.projectArea) not in projectArea %}
                        {% set projectArea = projectArea|merge([trnProjectRoomConfigurations.mstProjectArea.projectArea]) %}
                    {% endif  %}
                    {% if (trnProjectRoomConfigurations.areaValue) not in projectAreaValue %}
                        {% set projectAreaValue = projectAreaValue|merge([trnProjectRoomConfigurations.areaValue]) %}
                    {% endif  %}
                    {% if (trnProjectRoomConfigurations.agreementAmount) not in projectPrice %}
                        {#                {% set projectPrice = projectPrice|merge([trnProjectRoomConfigurations.agreementAmount~"|"~trnProjectRoomConfigurations.mstDenomination]) %}#}
                        {% set projectPrice = projectPrice|merge([trnProjectRoomConfigurations.agreementAmount]) %}
                        {% set projectPriceGroup = projectPriceGroup|merge({(trnProjectRoomConfigurations.mstDenomination.denomination):projectPrice|sort}) %}
                    {% endif  %}
                    {#            {% if attribute(aprices,"p_"~product.prdProductVariant[0].productVariantPrice) is not defined %}#}
                    {#                {% set aprices = aprices|merge({ ("p_"~product.prdProductVariant[0].productVariantPrice) : [product.prdProductVariant[0].productVariantPrice,product.prdProductVariant[0].productVariantPrice] }) %}#}
                    {#            {% endif %}#}

                {% endif  %}
        {% endfor %}
    {% endfor %}
    {% set projectMoreFilter = projectMoreFilter|merge({ 'Bathroom' : noOfBathRooms,'Property_Area':{'Area':projectArea,'AreaValue':projectAreaValue|sort} }) %}
